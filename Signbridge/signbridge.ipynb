   "source": [
    "import gradio as gr\n",
    "\n",
    "# 함수 정의\n",
    "def greet(name):\n",
    "    return f\"Hello, {name}!\"\n",
    "\n",
    "# 인터페이스 구성\n",
    "interface = gr.Interface(fn=greet, inputs=\"text\", outputs=\"text\")\n",
    "\n",
    "# 앱 실행\n",
    "interface.launch()"
   ]
  }
